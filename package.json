{
  "name": "progetto-tw",
  "version": "1.0.0",
  "description": "YMS - youtube music spider",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contimatteo/Progetto-TW.git"
  },
  "author": "Conti Matteo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/contimatteo/Progetto-TW/issues"
  },
  "homepage": "https://github.com/contimatteo/Progetto-TW#readme",
  "engines": {
    "node": "8.9.1"
  },
  "scripts": {
    "start": "node --harmony -r dotenv/config index.js",
    "nodemon": "./node_modules/nodemon/bin/nodemon.js index.js",
    "generate:models": "sh ./auto-generate-models.sh",
    "generate:migration": "node ./node_modules/sequelize-auto-migrations/bin/makemigration --name",
    "import:migration": "node ./node_modules/sequelize-auto-migrations/bin/runmigration",
    "preview:migration": "node ./node_modules/sequelize-auto-migrations/bin/makemigration --preview"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.1.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "jshint": "^2.9.6",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "sequelize": "^4.39.1",
    "sequelize-auto": "^0.4.29",
    "sequelize-auto-migrations": "^1.0.2",
    "sparql-client-2": "^0.6.3",
    "uglify-js": "^3.4.9",
    "youtube-node": "^1.3.2"
  },
  "devDependencies": {
    "nodemon": "^1.18.4",
    "request": "^2.88.0",
    "tape": "^4.7.0"
  }
}
